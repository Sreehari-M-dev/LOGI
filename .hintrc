{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not chrome <= 109",
    "not ios_saf <= 18.5",
    "not safari <= 18.5"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "button-type": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width",
          "scrollbar-color",
          "scrollbar-width: thin",
          "backdrop-filter"
        ]
      }
    ],
    "meta-viewport": "off",
    "no-inline-styles": "off"
  }
}